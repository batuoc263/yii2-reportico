{
	"name": "reportico/yii2-reportico",
	"description": "Reportico",
	"type": "yii2-extension",
	"keywords": ["yii2","extension","reportico","reports","reporting","database"],
	"license": "GPL-3.0",
	"authors": [
		{
			"name": "Peter Deed",
			"email": "info@reportico.org"
		}
	],
    "require": {
        "php": "^5.5.9|^7.0.0",
        "reportico-web/reportico": "^6.0"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "reportico\\reportico\\": ""
        }
    },
    "scripts": {
        "post-install-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ],
        "post-update-cmd": [
            "PhantomInstaller\\Installer::installPhantomJS"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
